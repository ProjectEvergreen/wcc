{
  "all": true,

  "include": [
    "src/**/**/*.js"
  ],

  "reporter": [
    "cobertura",
    "html",
    "text",
    "text-summary"
  ],

  "checkCoverage": true,

  "statements": 90,
  "branches": 85,
  "functions": 85,
  "lines": 90,

  "watermarks": {
    "statements": [75, 85],
    "branches": [75, 85],
    "functions": [75, 85],
    "lines": [75, 85]
  }
}